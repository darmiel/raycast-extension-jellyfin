{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "jellyfin",
  "title": "Jellyfin",
  "description": "Find Movies and Shows on your Jellyfin Instance",
  "icon": "command-icon.png",
  "author": "d2a",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Find Movies and Shows",
      "description": "Find Movies and Shows from your Jellyfin Instance and open them in the browser",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}